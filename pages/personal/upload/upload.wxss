page{
 overflow: hidden
}
.tables{
  width: 690rpx;
  height: auto;
  box-sizing: border-box;
  border-left: 1px solid #dbdbdb;
  border-right: 1px solid #dbdbdb;
  margin:0 auto;
  margin-top: 50rpx
}
.tables .row{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  border-top: 1px solid #dbdbdb
}
.tables .row:last-of-type{
  border-bottom: 1px solid #dbdbdb
}
.tables .row .tit{
  width: 194rpx;
  height: 100%;
  border-right: 1px solid #dbdbdb;
  font-family: PingFangSC-Medium;
  font-size: 26rpx;
  color: #2C2C2C;
  font-weight: bold;
  letter-spacing: -0.62rpx;
  line-height: 80rpx;
  text-align: center
}
.tables .row .content{
  width: 480rpx;
  height: 100%;
  line-height: 80rpx;
  box-sizing: border-box
}
.tables .row .content input{
  padding-left: 50rpx;
  width: 100%;
  height: 100%;
  font-size:24rpx;
  color: #5b5b5b;
  box-sizing: border-box
}
picker{
  height: 100%;
}
.selectInp{
  height: 80rpx !important;
  line-height: 80rpx !important
}
/* pages/ocr/ocr.wxss */
.view_show{
  display:block;
}
.view_hide{
  display:none;
}
.list{
  width: 100vw;
  height: 400rpx;
  border:1px solid #ddd;
  overflow: scroll;
 bottom: 0;
 left: 0;
 right: 0
}
.list .li{
  width: 100%;
  height: 80rpx;
  font-size: 24rpx;
  padding-left: 48rpx;
  box-sizing: border-box;
  background: #ddd !important;
  z-index: 99;
  border-bottom: 1px solid #fff
}
.submit{
  width: 440rpx;
  height: 80rpx;
  background: #4C80F5;
  border-radius: 100px;
  line-height: 80rpx;
  color: #fff;
  border: none;
  margin-top: 160rpx;
}