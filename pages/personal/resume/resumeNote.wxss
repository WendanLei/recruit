/* pages/personal/resume/resumeNote.wxss */
page{
  background: #f3f3f3;
}
.note{
  margin: 30rpx 20rpx;
}
.inputNote{
  position: relative;
  background: #EBEBEB;
  border:1px solid rgba(0,0,0,0.1);
  font-size: 28rpx;
}
.note textarea{
  width: 100%;
  height:253rpx; 
  padding: 20rpx;
}
.note text{
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
}
.note button{
  background: #6698FF;
  margin: 40rpx auto;
  width: 555rpx;
  color: #fff;
}
button::after{
    border: none;
}